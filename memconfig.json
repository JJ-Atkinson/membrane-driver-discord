{
  "expressions": {
    "guild": {
      "description": "Guild",
      "type": "text",
      "searchIn": "https://discord.com",
      "regex": "data-list-item-id=\"guildsnav___([0-9]{18})+"
    },
    "channel": {
      "description": "Channel",
      "type": "url",
      "searchIn": "https://discord.com",
      "regex": "https://discord.com/channels/([0-9]{18})+/([0-9]{18})+"
    },
    "user": {
      "description": "User",
      "type": "text",
      "searchIn": "https://discord.com",
      "regex": "/avatars/[0-9]+"
    },
    "message": {
      "description": "Channel Message",
      "type": "text",
      "searchIn": "https://discord.com",
      "regex": "id=\"chat-messages-[0-9]+-[0-9]+"
    }
  },
